{
  "parser": "babel-eslint",
  "extends": ["airbnb-base","plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-restricted-syntax": "off"
  }
}
